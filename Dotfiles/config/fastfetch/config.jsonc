{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small"
    },
    "display": {
        "separator": "  ",
        "size": {
            "ndigits": 0,
            "maxPrefix": "GB"
        },
        "key": {
            "type": "icon"
        }
    },
    "modules": [
	    {
		    "type": "os",
		    "keyColor": "yellow"
	    },
	    {
		    "type": "kernel",
		    "keyColor": "yellow" 
	    },
	    {
		    "type": "packages",
		    "keyColor": "yellow"
	    },
	    {
		    "type": "editor",
		    "keyColor": "yellow"
	    },
	    {
		    "type": "shell",
		    "keyColor": "yellow"
	    },
	    {
		    "type": "disk",
		    "keyColor": "green"
	    },
	    {
		    "type": "memory",
		    "keyColor": "green"
	    },
	    {
		    "type": "uptime",
		    "keyColor": "green"
	    } 
    ]
}
